{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
        "fetch": false
      },
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
        "react/forbid-prop-types": 0
    }
}